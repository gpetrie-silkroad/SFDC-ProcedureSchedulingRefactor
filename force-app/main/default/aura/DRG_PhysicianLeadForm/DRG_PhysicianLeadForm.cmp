                            <aura:set attribute="else">
                               <hr/>
                                <center>
                                    
                                    <h1>
                                        <u>
                                            <aura:text value="PHYSICIAN" />
                                        </u>
                                    </h1>    
                                </center>
                                 
                                <aura:if isTrue="{!v.hidePhysicianTable}" aura:id="renderPhysicianList">
