<aura:component controller="Procedure_LightningController" implements="flexipage:availableForAllPageTypes,lightning:actionOverride,force:hasRecordId" access="global">

	<aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
    <aura:dependency resource="markup://force:navigateToURL" type="EVENT"/>
    
    <!--%%%%%%%%%%%%%%%%%%%%%%%%%%%% ATTRIBUTE DECLARATION %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
    <aura:attribute name="newProcedureForm" type="Procedure_Form__c"
      							default="{ 'sobjectType': 'Procedure_Form__c', 'Name': '', 'Account__c': ''}"/>

	<aura:attribute name="procedureForms" type="Procedure_Form__c[]"/>
    
    <aura:attribute name="currentDate" type="Date" />
    <aura:attribute name="currentDateTime" type="DateTime" />
    
    <aura:attribute name="accounts" type="Account[]" />
    <aura:attribute name="account" type="Account"/>
    <aura:attribute name="physicians" type="Contact[]" />
    <aura:attribute name="proctors" type="Contact[]" />
    <aura:attribute name="stenters" type="Contact[]" />
    <aura:attribute name="cutdown" type="Contact[]" />

    <aura:attribute name="disUsers" type="User[]" />
    <aura:attribute name="amUsers" type="User[]" />
    <aura:attribute name="adUsers" type="User[]" />
    <aura:attribute name="tdsUsers" type="User[]" />
    <aura:attribute name="tdsUsers2" type="User[]" />
    <aura:attribute name="qualUsers" type="User[]" />
    <aura:attribute name="nullId" type="String" />
    
    <aura:attribute name="selectedAccount" type="String" />
    <aura:attribute name="selectedAccountName" type="String" />
    <aura:attribute name="selectedPrimaryPhysicianOperator" type="Contact"/>
    <aura:attribute name="selectedStenter" type="Contact"/>
    <aura:attribute name="selectedSRMProctor" type="Contact"/>
    <aura:attribute name="selectedCutdown" type="Contact"/>
    <aura:attribute name="addlPhys1" type="Contact" default="{'sobjectType' : 'Contact', 'Name': '', 'Account__c': ''}"/>
    <aura:attribute name="addlPhys2" type="Contact" default="{'sobjectType' : 'Contact', 'Name': '', 'Account__c': ''}"/>


    <aura:attribute name="currentUser" type="User" />

    <aura:attribute name="amUser" type="User" />
    <aura:attribute name="tdsUser" type="User" />
    <aura:attribute name="tdsUser2" type="User" />
    <aura:attribute name="adUser" type="User" />
    <aura:attribute name="qualUser" type="User" />

    <aura:attribute name="amUser_dis" type="Boolean" default="false" />
    <aura:attribute name="tdsUser_dis" type="Boolean" default="false" />
    <aura:attribute name="tdsUser2_dis" type="Boolean" default="false" />
    <aura:attribute name="adUser_dis" type="Boolean" default="false" />
    <aura:attribute name="qualUser_dis" type="Boolean" default="false" />
    <aura:attribute name="otherSrmUser_dis" type="Boolean" default="false" />



    <aura:attribute name="nowTime" type="String" />
    
    <aura:attribute name="disableTds2" type="Boolean" default="true" />
    <aura:attribute name="isLEX" type="Boolean" />
    <aura:attribute name="Desc" type="String" />
    <aura:attribute name="Name" type="String" />

    <aura:attribute name="reasonForAbort_dis" type="Boolean" default="true" />
    <aura:attribute name="noSRM" type="Boolean" default="false" />
    <aura:attribute name="noSrmPres_dis" type="Boolean" default="false" />
    <aura:attribute name="stentorAndCutdownRequired" type="Boolean" default="true" />

    <aura:attribute name="noPreCaseId" type="String" default="off" />
    <aura:attribute name="preCaseId" type="String" default="SRM-" />
    <aura:attribute name="disNoPreCaseId" type="Boolean" default="false" />
    <aura:attribute name="disPreCaseId" type="Boolean" default="false" />

    <aura:attribute name="contForm" type="Contact" default="{'sobjectType' : 'Contact'}"/>

    <aura:attribute name="simpleRecord" type="Object"/>
    <aura:attribute name="recordError" type="String"/>

    <aura:attribute name="hideReasonForTurnDown" type="Boolean" default="true" />
    
    <aura:attribute name="s2sStatedTime" type="String" default="00:00:00"/>
    <aura:attribute name="rftStatedTime" type="String" default="00:00:00"/>
    <aura:attribute name="ftStatedTime" type="String" default="00:00:00"/>
    
    <!-- EVENT SCHEDULING CONTROLLER -->
    <aura:attribute name="timeList" type="TimeConversionList__c[]" />
    <aura:attribute name="startTime" type="String" />
    <aura:attribute name="endTime" type="String" />
    <aura:attribute name="nowDate" type="Date" />

    <aura:attribute name="hospitalName" type="String" default="This is NOT WORKING!"/>
    <aura:attribute name="hospitalStreet" type="String"/>
    <aura:attribute name="hospitalCity" type="String"/>
    <aura:attribute name="hospitalTimeZone" type="String" />
    <aura:attribute name="hospitalState" type="String"/>
    <aura:attribute name="vendorCredentailCompany" type="String" />
    <aura:attribute name="locationInfo" type="String"/>
    <aura:attribute name="Description" type="String" />
    <aura:attribute name="OtherInformation" type="String" />
    <aura:attribute name="eventId1" type="String" />
    
    <aura:attribute name="newProcFormId" type="String" />
    
    
    <!-- RENDERING CONTROLLER VARIABLES-->
    <aura:attribute name="noRenderScheduler" type="Boolean" default="true" />
    <aura:attribute name="noRenderBottom" type="Boolean" default="true" />
    

    <force:recordData aura:id="recordLoader"
      recordId="{!v.recordId}"
      layoutType="FULL"
      targetRecord="{!v.simpleRecord}"
      targetFields="{!v.simpleRecord}"
      targetError="{!v.recordError}"
      mode="EDIT"
      recordUpdated="{!c.doInit}"
      />
    
      
    
          <c.ProcedureFormHeader/>

 <!--%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->  
    
    
    <!-- CREATE NEW PROCEDURE FORM -->
    <div aria-labelledby="newprocedureform">
        <!-- BOXED AREA -->
        <fieldset class="slds-box slds-theme--default slds-container--large">
            <legend id="newprocedureform" class="slds-text-heading--large slds-p-vertical--large">
                <br/>
              Add Procedure Form
                <center class="redsmall">Required Fields in Red</center>
            </legend>
            
            <!-- CREATE NEW PROCEDURE FORM -->    
            <form class="slds-form--stacked">  
				
				<aura:if isTrue="{! if(v.noRenderScheduler==true,false,true)}" >
                    <aura:set attribute="else" >
                        <center>
                            <lightning:button label="Submit New Procedure Form" class="slds-m-top--medium" variant="brand" onclick="{!c.clickCreate}"/>
                        </center>
                    </aura:set>
                </aura:if>
                <aura:if isTrue="{!v.noRenderScheduler}" >
                    <aura:set attribute="else" >
                        <center>
                            <lightning:button label="Schedule and Create Procedure Form" class="slds-m-top--medium" variant="brand" onclick="{!c.clickCreate}"/> <!-- onclick="{!c.onCreateSchedule}" /> -->
                        </center>
                    </aura:set>
                </aura:if>
                <br/>
                
                <!-- HEADER INFO TABLE -->
                <table columns="2" width="50%" >
                    <tr>
                        <td>
                            <aura:text value="Submitted By: " />

                        </td>
                        <td>
                            <aura:text aura:id="userName" value="{!v.currentUser.FirstName + ' ' + v.currentUser.LastName}"/>
                        </td>
                    </tr>
                    
                    <tr>
                        <td>
                            <aura:text value="Current Time is: " />
                        </td>
                        <td>
                            <aura:text value="{!v.nowTime}" />
                        </td>
                    </tr>
                </table>
                
                <br/>
                <!-- / HEADER INFO TABLE -->

                <table columns="2">                  
                    <tr>
                        <td width="20%" class="red">
                            <aura:text value="Account Site: " />
                        </td>
                        <td>
                            <aura:text aura:id="selAccount" value="{!v.account.IQMS_Customer_Number__c + ' - ' + v.account.Name}" /><br/>
                        </td>
                    </tr>
                    <tr>
                        <br/>
                    </tr>
                    <tr>
                        <td width="20%" class="red">
                            <aura:text value="Vendor Credential Co: " />
                        </td>
                        <td>
                            <aura:text aura:id="selAccount" value="{!v.account.Vendor_Credential_Company__c}" /><br/>
                        </td>
                    </tr>
                    <tr>
                        <br/>
                    </tr>
                    <tr>
                        <td width="20%" class="{!if(v.hideReasonForTurnDown == true, 'red' , '')}">
                            <aura:text value="Pre-Case Plan Id: " />
                        </td>
                        <td>
                            <div class="slds-box slds-theme_shade" >
                                <table columns="3">
                                    <tr class="valignCenter">
                                        <td width="50%" align="left">
                                        
                                            <lightning:input type="text" value="{!v.preCaseId}" maxlength="12" minlength="4" class="slds-form_inline" disabled="{!v.disPreCaseId}" onblur="{!c.onPreCaseIdEntry}"/>
                                                    
                                        </td>
                                        <td width="10%">
                                            <aura:text value=" " />
                                        </td>
                                        <td width="40%" align="right">
                                            <lightning:input type="checkbox" label="No Pre-Case" value="{!v.newProcedureForm.No_Pre_Case_Id__c}" aura:id="noPreCase" name="noPreCase" checked="{!v.newProcedureForm.No_Pre_Case_Id__c}" disabled="{!v.disNoPreCaseId}" onchange="{!c.clickNoPreCaseId}"/> 
                                        </td>
                                    </tr>
                                </table>
                            </div>
                        </td>
                    </tr>

                    <tr>
                        <br/>
                    </tr>
                    
                    <tr>
                        <td width="20%" class="red">
                            <aura:text value="Case Type: " />
                        </td>
                        <td>
                             <ui:inputSelect aura:id="proceduretype" label="" value="{!v.newProcedureForm.Procedure_Type__c}" change="{!c.onProcType}" >
                                    <ui:inputSelectOption text=""  label="- None Selected -"  />
                                    <ui:inputSelectOption text="TSP"  label="TSP" />
                                    <ui:inputSelectOption text="Commercial"  label="Commercial" />
                                    <ui:inputSelectOption text="ROADSTER 2"  label="ROADSTER 2" />
                                 	<ui:inputSelectOption text="ROADSTER 2 DW-MRI"  label="ROADSTER 2 DW-MRI" />
                                    <ui:inputSelectOption text="DW-MRI"  label="DW-MRI" />
                                    <ui:inputSelectOption text="C2R"  label="C2R" />	
                              		<ui:inputSelectOption text="Non-Cartid Use"  label="Non-Cartid Use" />
                                    <ui:inputSelectOption text="- T E S T -"  label="- T E S T -" />
                              </ui:inputSelect>
                           
                          	
                        </td>
                    </tr>
                    
                   	<tr>
                        <br/>
                    </tr>
                                       
                </table>
                
                <table columns="2">
                    
                    <tr>
                        <td width="20%" class="red">
                      
                             <aura:text value="Procedure Status: " />
                        </td>
                        <td>
                            <div class="slds-box slds-theme_shade">
                                <table columns="3">
                                     <ui:inputSelect aura:id="completion" label="" value="{!v.newProcedureForm.Procedure_Completion__c}" change="{!c.onCompl}" >
                                        <ui:inputSelectOption text=""  label="- None Selected -"  />
                                        <ui:inputSelectOption text="Scheduled"  label="Scheduled" />
                                        <ui:inputSelectOption text="Completed"  label="Completed" />
                                        <ui:inputSelectOption text="Converted to CEA"  label="Converted to CEA" />
                                        <ui:inputSelectOption text="Converted to TF-CAS"  label="Converted to TF-CAS" />
                                        <ui:inputSelectOption text="Aborted"  label="Aborted" />
                                        <!--<ui:inputSelectOption text="Cancelled/Rescheduled"  label="Cancelled/Rescheduled" />-->
                                        <ui:inputSelectOption text="Procedure Turned Down" label="Procedure Turned Down" />
                                    </ui:inputSelect>
                                    <tr>
                                       <aura:if isTrue="{! if(v.noRenderScheduler == true, false, true) }" >
                                            <aura:set attribute="else">
                                                <td colspan="3"> 
                                                    <lightning:input aura:id="reasonforabort" label="Reason for Cancellation/Rescheduled, Abort, or Conversion" disabled="{!v.reasonForAbort_dis}" name="reasonforabort" class="inputArea" value="{!v.newProcedureForm.Reason_Aborted__c}" />                                          
                                                </td>
                                            </aura:set>
                                    	</aura:if>
                                    </tr>
                                </table>
                            </div>
                        </td>
                    </tr>
                </table>




                <aura:if isTrue="{!v.hideReasonForTurnDown}" aura:id="reasonForTurnDown" >
                    <aura:set attribute="else">
                        <br/>
                        <table columns="2">
                            <tr>
                                <td width="20%" class="red">
                                    <aura:text value="Reason for Turning Procedure Down" />
                                </td>
                                        <ui:inputSelect aura:id="completion" label="" value="{!v.newProcedureForm.Reason_for_Turning_Procedure_Down__c}" >
                                            <ui:inputSelectOption text=""  label="- None Selected -"  />
                                            <ui:inputSelectOption text="Not enough distance between access and disease"  label="Not enough distance between access and disease"  />
                                            <ui:inputSelectOption text="Lesion morphology: Too highly calcified"  label="Lesion morphology: Too highly calcified"  />
                                            <ui:inputSelectOption text="Lesion morphology: Thrombus"  label="Lesion morphology: Thrombus"  />
                                            <ui:inputSelectOption text="Disease in the common"  label="Disease in the common"  />
                                            <ui:inputSelectOption text="Lack of high surgical risk"  label="Lack of high surgical risk"  />
                                            <ui:inputSelectOption text="String sign"  label="String sign"  />
                                            <ui:inputSelectOption text="Age – patient too young for a stent"  label="Age – patient too young for a stent"  />
                                            <ui:inputSelectOption text="Intracranial disease"  label="Intracranial disease"  />
                                            <ui:inputSelectOption text="Patient turned down"  label="Patient turned down"  />
                                            <ui:inputSelectOption text="Diameter of ICA or common either too small or too large"  label="Diameter of ICA or common either too small or too large"  />
                                            <ui:inputSelectOption text="Failure to be on DAPT"  label="Failure to be on DAPT"  />
                                            <ui:inputSelectOption text="Other"  label="Other"  />
                                        </ui:inputSelect> 
                                <td>

                                </td>
                            </tr>
                            <aura:if isTrue="{!if(v.newProcedureForm.Reason_for_Turning_Procedure_Down__c == 'Other', false, true)}" aura:id="reasonForTurnDownOther" >
                                <aura:set attribute="else">
                                    <tr>
                                        <td width="20%" class="red">
                                            <aura:text value="Other Reason (Required)" />
                                        </td>
                                        <td>
                                            <lightning:input aura:id="commentsReasonForTurnDownOther" label=""  name="commentsReasonForTurnDownOther" class="inputArea"  value="{!v.newProcedureForm.Comments__c}" />  
                                        </td>
                                    </tr>
                                </aura:set>
                            </aura:if>
                        </table>
                    
                        <table columns="2">
                            <tr>
                                <td width="20%" class="red">
                                        <aura:text value="Physician Primary Operator: " />
                                </td>
                                <td>
                                    
                                    <ui:inputSelect aura:id="primaryPhysicianOperator" label="" value="{!v.selectedPrimaryPhysicianOperator}" change="{!c.updatePrimaryPhysicianOperator}" >
                                        
                                        <aura:iteration items="{!v.cutdown}" var="item">
                                            <ui:inputSelectOption text="{!item.Id}" label="{!item.FirstName + ' ' + item.MiddleName + ' ' +  item.LastName + ' - ' + item.Account.Name}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                        </table>
                    </aura:set>
                </aura:if>

                <table>
                    <aura:if isTrue="{!v.noRenderScheduler}">
                    	<aura:set attribute="else" >
                            <tr>
                                <td colspan="3">
                                    <hr/>
                                    <center><h1><u>SCHEDULING INFORMATION</u></h1></center>
                                    <br/>
                                </td>
                            </tr>
                        </aura:set>
                    </aura:if>

                    <tr>
                        <td width="20%" class="red">
                            <br/>
                            <aura:text value="Procedure Date: " />
                        </td>
                        <td>
                            <div>
                				<lightning:input aura:id="proceduredate" label="" type="Date"  name="proceduredate" value="{!v.newProcedureForm.Procedure_Date__c}" />  
                			</div>
                        </td>
                    </tr> 

                    <aura:if isTrue="{!v.noRenderScheduler}">
                    	<aura:set attribute="else" >
                            
                            <br/>
                            <tr>
                                <td width="20%" class="red">
                                    <aura:text value="Scheduled Time Zone: " />
                                </td>       
                                <td>
                                    <ui:inputSelect value="{!v.hospitalTimeZone}" aura:id="localTimeZone" label="" >
                                        <ui:inputSelectOption label="Eastern" text="America/New_York"/>
                                        <ui:inputSelectOption label="Central" text="America/Chicago"/>
                                        <ui:inputSelectOption label="Mountain" text="America/Denver"/>
                                        <ui:inputSelectOption label="Pacific" text="America/Los_Angeles"/>
                                        <ui:inputSelectOption label="Arizona" text="America/Phoenix"/>
                                        <ui:inputSelectOption label="Alaska" text="America/Anchorage"/>
                                        <ui:inputSelectOption label="Hawaii" text="Pacific/Honolulu"/>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            <br/>
                            <tr>
                                <td width="20%" class="red">
                                    <aura:text value="Scheduled Start Time: " />
                                </td>
                                <td>
                                    <ui:inputSelect value="{!v.startTime}"  change="{!c.resetEndTime}" aura:id="localStartTime" >
                                        <aura:iteration aura:id="startTimeI" items="{!v.timeList}" var="tl">
                                                <ui:inputSelectOption aura:id="stTm" text="{!tl.Time24__c}" label="{!tl.Name}" disabled="{!if(greaterthanorequal(tl.Time24__c, '23:30'),true,false)}"/>
                                            </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            
                        
                            <br/>
                        
                            <tr>
                                <td width="20%" class="red">
                                    <aura:text value="Scheduled End Time: " />
                                </td>
                                <td>
                                    <ui:inputSelect value="{!v.endTime}" aura:id="localEndTime">
                                        <aura:iteration aura:id="endTimeI" items="{!v.timeList}" var="tl">
                                                <ui:inputSelectOption aura:id="endTm" disabled="{!if(lessthanorequal(tl.Time24__c, v.startTime),true,false)}" text="{!tl.Time24__c}" label="{!tl.Name}"/>
                                            </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            <tr>
                                <td width="20%"  class="red">
                                    <aura:text value="Location" />
                                    <br/>
                                    <aura:text value="Information: " />
                                </td>
                                <td>
                                   	<lightning:textarea aura:id="locinfo" label=""  name="locinfo" value="{!v.locationInfo}" class="textMultiline" />
                                </td>
                            </tr>
                            <br/>
                            <tr>
                                <td colspan="2">
                                    <hr/>
                                    <center><h1><u>SRM PERSONNEL PRESENT AT PROCEDURE</u></h1></center>
                                        <center class="redsmall">Either No SRM Personnel must be checked <br/> or <br/> at least one SRM personnel selected</center>
                                    <br/>
                                </td>
                            </tr>
                            <tr>
                                <td width="20%" >
                                </td>    
                                <td class="red">    
                                    <lightning:input type="checkbox" label="No SRM Personnel Present" aura:id="noSrmPres" name="noSrmPres"  disabled="{!v.noSrmPres_dis}" onchange="{!c.disableUsersPres}" checked="{!v.newProcedureForm.No_SRM_Personnel_Present__c}" value="{!v.newProcedureForm.No_SRM_Personnel_Present__c}" />	
                                </td>
                            </tr>
                            <br/>
                            <tr>
                                <td width="20%" class="{!if(!v.noSRM,'red','')}" >
                                    <aura:text value="Primary AM:" />
                                </td>
                                <td>
                                   	<ui:inputSelect aura:id="amSelect" label="" class="selectwide" value="{!v.amUser}" change="{!c.updateAMUser}" disabled="{!v.amUser_dis}"  >
                    					<aura:iteration aura:id="amInt" items="{!v.amUsers}" var="amU">
                        					<ui:inputSelectOption aura:id="amsel" text="{!if(amU.FirstName == '- None ', 'none', amU.Id)}" disabled="false" label="{!amU.FirstName + ' ' + amU.LastName}"/>
                    					</aura:iteration>
                					</ui:inputSelect>
                                </td>
                            </tr>
                            <br/>
                            <tr>
                             	<td width="20%">
                        			<aura:text value="Primary TDS: " />
                        		</td>
                                <td>
                                    <ui:inputSelect aura:id="tdsSelect" label="" class="selectwide" value="{!v.tdsUser}" change="{!c.updateTDSUser}" disabled="{!v.tdsUser_dis}" >
                                        <aura:iteration items="{!v.tdsUsers}" var="tdsU">
                                            <ui:inputSelectOption aura:id="amsel" text="{!if(tdsU.FirstName == '- None ' || tdsU.FirstName ==  '- DISABLED -', 'none', tdsU.Id)}" disabled="false" label="{!tdsU.FirstName + ' ' + tdsU.LastName}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            <br/>



                            <tr>
                                <td width="20%" >
                                    <aura:text value="Secondary TDS: " />
                                </td>
                                
                                <td>
                                    <ui:inputSelect aura:id="tdsSelect2" label="" class="selectwide" value="{!v.tdsUser2}" change="{!c.updateTDSUser2}" disabled="{!v.disableTds2}" >
                                        <aura:iteration items="{!v.tdsUsers2}" var="tdsU2">
                                            <ui:inputSelectOption text="{!if(tdsU2.FirstName == '- None ' || tdsU2.FirstName ==  '- DISABLED -', 'none', tdsU2.Id)}" label="{!tdsU2.FirstName + ' ' + tdsU2.LastName}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            
                            <tr>
                                <br/>
                            </tr>
                            <tr>
                                <td width="20%" class="{!if(!v.noSRM,'red','')}">
                                    <aura:text value="Other Qualified SRM: " />
                                </td>
                                <td>
                                    <ui:inputSelect aura:id="qualSelect" label="" class="selectwide" value="{!v.qualUser}" change="{!c.updateQualUser}" disabled="{!v.qualUser_dis}">
                                        <aura:iteration items="{!v.qualUsers}" var="qualU">
                                            <ui:inputSelectOption text="{!if(qualU.FirstName == '- None ' || qualU.FirstName ==  '- DISABLED -', 'none', qualU.Id)}" label="{!qualU.FirstName + ' ' + qualU.LastName}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            <br/>
                            <tr>                     
                                <td width="20%" class="{!if(!v.noSRM,'red','')}">
                                    <aura:text value="AD: " />
                                </td>
                                <td>
                                      <ui:inputSelect aura:id="adSelect" label="" class="selectwide" value="{!v.adUser}" change="{!c.updateADUser}" disabled="{!v.adUser_dis}">
                                        <aura:iteration items="{!v.adUsers}" var="adU">
                                            <ui:inputSelectOption text="{!if(adU.FirstName == '- None ', 'none',adU.Id)}" label="{!adU.FirstName + ' ' + adU.LastName}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            <tr>                     
                                <td width="20%">
                                    <aura:text value="Other SRM Personnel Present: " />
                                </td>
                                <td>
                                    <lightning:input aura:id="othSRM" label="" class="selectwide" value="{!v.newProcedureForm.Other_SRM_Personnel_Present__c__c}" disabled="{!v.otherSrmUser_dis}"/>
                                        
                                </td>
                            </tr>


                            <tr>
                                <br/>
                            </tr>


                            <tr>
                                <td colspan="2">
                                    <hr/>
                                        <center><h1><u>PRIMARY PHYSICIAN</u></h1></center>
                                </td>
                            </tr>
                            <tr>
                                <br/>
                            </tr>





                            <tr
                            >
                                <td width="20%" class="red">
                                        <aura:text value="Primary Physician: " />
                                </td>
                                <td>
                            
                                    <ui:inputSelect aura:id="primaryPhysicianOperator" label="" value="{!v.selectedPrimaryPhysicianOperator}" change="{!c.updatePrimaryPhysicianOperator}" >
                                        
                                        <aura:iteration items="{!v.cutdown}" var="item">
                                            <ui:inputSelectOption text="{!item.Id}" label="{!item.FirstName + ' ' + item.MiddleName + ' ' +  item.LastName + ' - ' + item.Account.Name}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </td>
                            </tr>
                            <tr>
                                <br/>
                            </tr>
